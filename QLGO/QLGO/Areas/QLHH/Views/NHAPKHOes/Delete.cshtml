@model QLGO.Models.NHAPKHO

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Xóa</h2>

<h3>Bạn muốn xóa cái này chứ?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Ngày nhập kho
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NgayNhapKho)
        </dd>

        <dt>
            Tên người dùng
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NGUOIDUNG.TenND)
        </dd>

        <dt>
            Tên nhà cung cấp
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NHACUNGCAP.TenNCC)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" />
            @Html.ActionLink("Quay lại", "Index")
        </div>
    }
</div>
