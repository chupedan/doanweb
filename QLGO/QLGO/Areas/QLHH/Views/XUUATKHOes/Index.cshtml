@model IEnumerable<QLGO.Models.XUUATKHO>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Mã phiếu xuất
        </th>
        <th>
            Ngày xuất kho
        </th>
        <th>
            Tên người dùng
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IDPhieuXuat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NgayXuatKho)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NGUOIDUNG.TenND)
            </td>
            <td>
                @Html.ActionLink("Sửa", "Edit", new { id = item.IDPhieuXuat }) |
                @Html.ActionLink("Xem chi tiết", "Details", new { id = item.IDPhieuXuat }) |
                @Html.ActionLink("Xóa", "Delete", new { id = item.IDPhieuXuat })
            </td>
        </tr>
    }

</table>
